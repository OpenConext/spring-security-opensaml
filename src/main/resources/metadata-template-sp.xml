<?xml version="1.0"?>
<md:EntityDescriptor xmlns:md="urn:oasis:names:tc:SAML:2.0:metadata" xmlns:mdui="urn:oasis:names:tc:SAML:metadata:ui"
    validUntil="%VALID_UNTIL%"
    entityID="%ENTITY_ID%">
    <md:SPSSODescriptor protocolSupportEnumeration="urn:oasis:names:tc:SAML:2.0:protocol">
        <md:NameIDFormat>urn:oasis:names:tc:SAML:2.0:nameid-format:persistent</md:NameIDFormat>
        <md:AssertionConsumerService 
            Binding="urn:oasis:names:tc:SAML:2.0:bindings:HTTP-POST" 
            Location="%ASSERTION_CONSUMER_SERVICE_URL%" index="1"/>
        <md:AttributeConsumingService index="1">
            <md:ServiceName xml:lang="en">%SERVICE_NAME_EN%</md:ServiceName>
            <md:ServiceName xml:lang="nl">%SERVICE_NAME_NL%</md:ServiceName>
            <md:ServiceDescription xml:lang="en">%SERVICE_DESCRIPTION_EN%</md:ServiceDescription>
            <md:ServiceDescription xml:lang="nl">%SERVICE_DESCRIPTION_NL%</md:ServiceDescription>
        </md:AttributeConsumingService>
    </md:SPSSODescriptor>
    <md:ContactPerson contactType="administrative">
        <md:GivenName>%CONTACT_PERSON_ADMINISTRATIVE_GIVEN_NAME%</md:GivenName>
        <md:SurName>%CONTACT_PERSON_ADMINISTRATIVE_SUR_NAME%</md:SurName>
        <md:EmailAddress>%CONTACT_PERSON_ADMINISTRATIVE_EMAIL%</md:EmailAddress>
    </md:ContactPerson>
    <md:ContactPerson contactType="technical">
        <md:GivenName>%CONTACT_PERSON_TECHNICAL_GIVEN_NAME%</md:GivenName>
        <md:SurName>%CONTACT_PERSON_TECHNICAL_SUR_NAME%</md:SurName>
        <md:EmailAddress>%CONTACT_PERSON_TECHNICAL_EMAIL%</md:EmailAddress>
    </md:ContactPerson>
    <md:ContactPerson contactType="support">
        <md:GivenName>%CONTACT_PERSON_SUPPORT_GIVEN_NAME%</md:GivenName>
        <md:SurName>%CONTACT_PERSON_SUPPORT_SUR_NAME%</md:SurName>
        <md:EmailAddress>%CONTACT_PERSON_SUPPORT_EMAIL%</md:EmailAddress>
    </md:ContactPerson>    
</md:EntityDescriptor>
